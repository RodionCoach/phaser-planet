{
  "name": "math",
  "version": "0.1.0",
  "description": "Aristek game",
  "main": "src/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "start": "rollup -cw",
    "verify": "run-p verify:ts verify:eslint verify:prettier",
    "verify:commit": "run-s verify:ts verify:staged",
    "verify:ts": "tsc --noEmit",
    "verify:eslint": "eslint --max-warnings 0 \"src/**/*.{js,ts}\"",
    "verify:prettier": "prettier --check \"src/**/*.{js,ts}\"",
    "verify:staged": "lint-staged"
  },
  "repository": {
    "type": "git"
  },
  "author": "ARISTEK",
  "license": "MIT",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts}": [
      "eslint",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn verify:commit",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": "@commitlint/config-conventional"
  },
  "dependencies": {
    "phaser": "3.50.0",
    "phaser3-rex-plugins": "1.1.41"
  },
  "devDependencies": {
    "@babel/core": "7.13.14",
    "@babel/preset-env": "7.4.5",
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-conventional": "12.1.1",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "babel-eslint": "10.1.0",
    "cross-env": "7.0.3",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "4.2.5",
    "lint-staged": "10.5.4",
    "npm-run-all": "4.1.5",
    "path": "0.12.7",
    "phaser3-rex-plugins-types": "^0.1.3",
    "prettier": "2.2.1",
    "rollup": "2.35.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-livereload": "2.0.0",
    "rollup-plugin-serve": "1.1.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.29.0",
    "typescript": "4.2.3"
  }
}
